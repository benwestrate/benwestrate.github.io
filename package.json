{
  "name": "benwestrate.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "styles": "node-sass public/stylesheets/style.scss public/stylesheets/style.css --source-map true",
    "html": "pug views/ --out ./ -P",
    "build": "npm run styles && npm run html",
    "watch": "onchange './**/*.pug' 'public/**/*.scss' -- npm run build",
    "prewatch": "npm run build"
  },
  "dependencies": {
    "less": "^2.6.1",
    "node-sass": "^4.11.0",
    "onchange": "^5.2.0",
    "pug": "^2.0.0-beta4",
    "reset-css": "^4.0.1",
    "watch": "^0.19.1"
  }
}
